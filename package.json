  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@iconify-json/bi": "^1.1.1",
    "@iconify-json/mdi": "^1.1.9",
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "^2.3.0",
    "@windicss/plugin-scrollbar": "^1.2.3",
    "typescript": "^4.5.4",
    "unplugin-icons": "^0.14.1",
    "unplugin-vue-components": "^0.18.5",
    "vite": "^2.9.0",
    "vite-plugin-windicss": "^1.8.3",
    "vue-openweather": "^0.0.6",
    "vue-tsc": "^0.29.8",
    "windicss": "^3.5.1"
  }
