{
  "name": "vue-openweather",
  "version": "0.0.7",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-openweather.umd.js",
  "module": "./dist/vue-openweather.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-openweather.es.js",
      "require": "./dist/vue-openweather.umd.js"
